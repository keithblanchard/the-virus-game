{
  "name": "dot-game",
  "version": "0.1.0",
  "description": "Example project showing how to use Sass middleware",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "sass": "node ./node_modules/sass/sass.js ./scss/index.scss ./public/index.css",
    "build": "babel src/ -d public/",
    "prepublish": "yarn run build"
  },
  "dependencies": {
    "express": "^4.15.0",
    "sass": "^1.17.3"
  },
  "keywords": [
    "node",
    "express",
    "sass"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-flow": "^7.9.0",
    "flow-bin": "^0.121.0"
  }
}
